                    <use xlink:href="#svg-public"></use>
                </svg>
                <!-- /TAG STICKER ICON -->
            </div>
            <!-- /TAG STICKER -->
            <!-- PROFILE HEADER INFO ACTIONS -->
            <div class="profile-header-info-actions">
                <!-- PROFILE HEADER INFO ACTION -->
                @if (ViewBag.user != null)
                {
                    @if (ViewBag.joined == false)
                    {
                        <a class="profile-header-info-action button secondary" @*id="joining"*@ asp-action="joingroup" asp-route-id="@Model.Id">
                            <!-- ICON JOIN GROUP -->
                            <i class="fas fa-user-plus" style="font-size:22px; margin-top:13px;"></i>
                            <!-- /ICON JOIN GROUP -->
                        </a>
                    }
                    else
                    {
                        <a class="profile-header-info-action button bg-danger" asp-action="leavegroup" asp-route-id="@ViewBag.grouphub.Id">
                            <!-- ICON JOIN GROUP -->
                            <i class="fas fa-user-minus" style="font-size:22px; margin-top:13px;"></i>
                            <!-- /ICON JOIN GROUP -->
                        </a>
                    }
                }

                <div id="messagesList"></div>

                <a class="profile-header-info-action button">
                    <!-- ICON MORE DOTS -->
                    <svg class="icon-more-dots">
                        <use xlink:href="#svg-more-dots"></use>
                    </svg>
                    <!-- /ICON MORE DOTS -->
                </a>
                <!-- /PROFILE HEADER INFO ACTION -->
            </div>
            <!-- /PROFILE HEADER INFO ACTIONS -->
